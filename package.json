{
  "private": true,
  "scripts": {
    "dev": "npm run development",
    "development": "mix",
    "watch": "mix watch",
    "watch-poll": "mix watch -- --watch-options-poll=1000",
    "hot": "mix watch --hot",
    "hotssl": "MIX_HOT_HTTPS=true mix watch --hot",
    "prod": "npm run production",
    "production": "mix --production",
    "test": "jest",
    "tdd": "jest --watch"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^6.3.0",
    "@vue/test-utils": "^1.2.2",
    "axios": "^1.3",
    "axios-mock-adapter": "^1.21.2",
    "babel-core": "^7.0.0-0",
    "babel-jest": "^27.2.1",
    "bootstrap": "^4.6.0",
    "bootstrap-vue": "^2.23.1",
    "jest": "^29.4.2",
    "jest-environment-jsdom": "^29.4.2",
    "jquery": "^3.6",
    "laravel-mix": "^6.0.49",
    "laravel-mix-valet": "^1.4.0",
    "lodash": "^4.17.19",
    "popper.js": "^1.16.1",
    "postcss": "^8.1.14",
    "resolve-url-loader": "^5.0.0",
    "sass": "^1.32.11",
    "sass-loader": "^11.0.1",
    "vue": "^2.6.12",
    "vue-jest": "^3.0.7",
    "vue-loader": "^15.9.8",
    "vue-template-compiler": "^2.6.12"
  },
  "dependencies": {
    "intersection-observer": "^0.12.2",
    "moment": "^2.29.4",
    "numeral": "^2.0.6",
    "slugify": "^1.6.5",
    "urijs": "^1.19.11",
    "vue-clipboard2": "^0.3.3",
    "vue-multiselect": "^2.1.7",
    "vue-phone-number-input": "^1.12.13",
    "vue-router": "^3.6.5",
    "vue-template-compiler": "^2.6.14",
    "vuex": "^3.6.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      ".*\\.(vue)$": "vue-jest",
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/resources/js/$1"
    },
    "setupFiles": [
      "<rootDir>/resources/js/tests/setup.js"
    ],
    "testEnvironment": "jsdom"
  }
}
